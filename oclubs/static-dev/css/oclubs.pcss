:root {
	--color-lightgrey: #f2f2f2;

	--color-lightgrey-prop: {
		color: var(--color-lightgrey);
	};

	--fancy-font: 'Kalam';
	--serious-font: var(--fancy-font);
}

a, a:hover, a:focus {
	text-decoration: none;
}

.color-lightgrey {
	@apply --color-lightgrey-prop;
}

body {
	background: rgb(1,39,58);
	background: linear-gradient(to right, rgba(1,39,58,1) 0%, rgba(2,103,154,1) 50%, rgba(1,39,58,1) 100%);

	font-family: var(--fancy-font);
}

