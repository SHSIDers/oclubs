	border: 1px solid #a6a6a6;
	padding: 0.25em 0;
	float: left;
	margin-top: 2.4em;
	background-color: var(--black);

	& input {
		&[type="text"] {
			width: 88%;
			outline: none;
			padding: 0em 0.5em;
			font-size: 1em;
			color: var(--darkgrey);
			position: relative;
			border: none;
			background-color: var(--black);
		}

		&[type="submit"] {
			border: none;
			height: 26px;
			width: 26px;
			display: inline-block;
			position: absolute;
			outline: none;
			cursor: pointer;
			top: -5px;
			right: -4px;
			background-color: transparent;
		}
	}

	@media screen and (max-width: 992px) {
		width: 18%;
	}

	@media screen and (max-width: 600px) {
		width: 40%;
		margin-top: 2em;
		margin-left: 2em
	}
	@media screen and (max-width: 500px) {
		margin-top: 1.5em;
		margin-left: 1em;
	}
	@media screen and (max-width: 400px) {
		margin-left: 0;
	}
}

.blockpic {
	padding: 0;
	height: 300px;
	margin: 0px;
	float: left;
	border: 0px solid var(--white);
	overflow: hidden;
	position: relative;
	text-align: center;
	box-shadow: 1px 1px 2px var(--lightgrey);
	cursor: default;

	& .mask {
		width: 100%;
		height: 100%;
		position: absolute;
		overflow: hidden;
		left: 0;
		padding-top: 115px;
		background-color: rgba(25, 140, 204, 0.55);
		top: -66.66%;
		opacity: 0;
		transition: all 0.3s ease-out 0.5s;
	}

	& .img {
		display: block;
		position: relative;
		min-width: 100%;
		min-height: 100%;
		background-position: center;
		background-size: cover;
	}

	& h2 {
		@apply --color-white-prop;

		text-transform: uppercase;
		text-align: center;
		position: relative;
		font-size: 1.21em;
		padding: 0 0 10px 0;
		margin: 0;
		transition: all 0.2s ease-in-out 0.1s;
	}

	& p {
		@apply --color-white-prop;

		/* font-family: Georgia, serif; */
		font-style: italic;
		font-size: 0.86em;
		position: relative;
		padding: 0;
		text-align: center;
		transition: all 0.2s ease-in-out 0.2s;
	}

	& h2, & p {
		transform: translateY(-66.6%);
	}

	&:hover {
		& .mask {
			opacity: 1;
			top: 0px;
			transition-delay: 0s;
			animation: bounceY 0.9s linear;
		}

		& h2, & p {
			transform: translateY(0px);
		}

		& h2 {
			transition-delay: 0.4s;
		}

		& p {
			transition-delay: 0.2s;
		}
	}

	@media screen and (max-width: 992px) {
		&, & .mask {
			height: 250px;
		}

		& .mask {
			padding-top: 90px;
		}
	}

	@media screen and (max-width: 720px) {
		&, & .mask {
			height: 200px;
		}

		& .mask {
			padding-top: 65px;
		}
	}

	@media screen and (max-width: 600px) {
		&, & .mask {
			height: 160px;
			font-size: 90%;
		}

		& .mask {
			padding-top: 48.5px;
		}
	}

	@media screen and (max-width: 525px) {
		&, & .mask {
			height: 130px;
			font-size: 80%;
		}

		& .mask {
			padding-top: 37px;
		}
	}

	@media screen and (max-width: 400px) {
		&, & .mask {
			height: 100px;
			font-size: 70%;
		}
		& .mask {
			padding-top: 25px;
		}
	}
}

@keyframes bounceY {
	0% { transform: translateY(-68.3%);}
	40% { transform: translateY(-33.3%);}
	65% { transform: translateY(-17.3%);}
	82% { transform: translateY(-8.33%);}
	92% { transform: translateY(-4%);}
	55%, 75%, 87%, 97%, 100% { transform: translateY(0px);}
}
