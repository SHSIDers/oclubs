:root {
	--white: #f2f2f2;
	--lightgrey: #e2e2e2;
	--grey: #ccc;
	--darkgrey: #999; /* foreground of header & sidenav */
	--black: #222;

	--blue: #337ab7;

	--color-white-prop: {
		color: var(--white);
	};

	--bg-white-hover-grey: {
		background-color: var(--white);

		&:hover {
			background-color: var(--grey);
		}
	};

	--bg-white-border-lightgrey: {
		background-color: var(--white);
		border: 1px solid var(--lightgrey);
	};

	--color-white-hover-grey: {
		color: var(--white);

		&:hover {
			color: var(--grey);
		}
	};

	--fancy-font: 'Kalam';
	--serious-font: var(--fancy-font);
}

a, a:hover, a:focus {
	text-decoration: none;
}

body {
	background: rgb(1,39,58);
